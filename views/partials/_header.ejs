  <title>TinyApp</title>

  <% if (username) {  %>
    Currently using: <%= username %>
  <form action="/logout" method="POST" style="margin:5px;">
  <input type="submit" value="logout">
  </form>
  <% } else { %>

    <form action="/login" method="POST" style="margin:5px;">
      <label for="username">Name:</label>
      <input id="username" type="text" name="username" placeholder="Enter username" style="width: 100px">
      <input type="submit" value="Submit">
    </form>

  <% } %>